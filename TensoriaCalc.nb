 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox[
     OverscriptBox["\[Epsilon]", "~"], 
     RowBox[{
      RowBox[{
       SubscriptBox["\[Mu]", "1"], "..."}], 
      SubscriptBox["\[Mu]", "d"]}]], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"|", 
        RowBox[{"det", " ", 
         SubscriptBox["g", "\[Alpha]\[Beta]"]}], "|"}], ")"}], 
      RowBox[{"1", "/", "2"}]], 
     SubscriptBox["\[Epsilon]", 
      RowBox[{
       RowBox[{
