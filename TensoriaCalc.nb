Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"LieDerivative", "[", 
     RowBox[{"\[Xi]_Tensor", ",", "m_Tensor"}], "]"}], "/;", 
    RowBox[{
     RowBox[{"CheckMetricTensor", "[", "m", "]"}], "&&", 
